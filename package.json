  "scripts": {
    "validate:dev": "cross-env DEPLOY_ENV=development.local node prebuild.js",
    "validate:production": "cross-env DEPLOY_ENV=production node prebuild.js",
    "start:http": "yarn validate:dev && cross-env PORT=10889 react-app-rewired start",
    "start:https": "yarn validate:dev && cross-env HTTPS=true PORT=10888 react-app-rewired start",
    "tunnel": "ngrok http --region eu 10889",
    "build": "yarn validate:production && react-app-rewired build",
    "test": "react-app-rewired test",
    "eject": "react-app-rewired eject",
    "deploy": "vk-miniapps-deploy",
    "rollout": "yarn build && yarn deploy"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
